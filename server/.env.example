# Copy to .env and set these values. Do NOT commit real secrets.
PORT=8787

# Dune analytics API
DUNE_API_KEY=

# Supabase configuration
# Use a service role key on the server if you need unrestricted inserts/updates, else anon key is ok for reads.
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
# Alternatively, if you only have anon key:
# SUPABASE_ANON_KEY=

# Flow access node (for future smart contract calls / Forte integration)
FLOW_ACCESS_API=https://rest-mainnet.onflow.org

# Optional Beezie/Gemini proxy
GEMINI_API_KEY=
